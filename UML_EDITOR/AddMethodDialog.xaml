<Window x:Class="Lab5.AddMethodDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab5"
        mc:Ignorable="d"
        Title="Имя метода" Height="180" Width="260">
    <Grid>
        <Label Content="Введите имя метода:" Margin="20,0,0,0" FontWeight="DemiBold"/>
        <Grid Margin="0,30,0,0">
            <Button Content="OK" Click="Button_Click" Width="90" Height="30" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="20,0,20,20" Background="#FF62D6C1" BorderBrush="#FFEA45DB" Foreground="#FF253242" RenderTransformOrigin="0.5,0.5"/>
            <TextBox x:Name="TXT_DIALOG" Width="200" Height="34" FontSize="21" Text="2323" TextAlignment="Center" VerticalAlignment="Top" Margin="20,0,20,20"/>
        </Grid>
    </Grid>
</Window>
